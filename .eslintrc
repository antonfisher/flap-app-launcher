{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "ecmaVersion": 6,
  "rules": {
    "no-plusplus": 0,
    "comma-dangle": 0,
    "block-spacing": 0,
    "spaced-comment": 0,
    "no-param-reassign": 0,
    "object-curly-spacing": 0,
    "max-len": [
      "error",
      120
    ]
  }
}
